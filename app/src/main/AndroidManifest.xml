<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carica.contratclient_v1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MySocket"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SingupActivity"
            android:label="@string/title_activity_singup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShowContact"
            android:allowBackup="true"
            android:supportsRtl="true"
            android:theme="@style/AppTheme" />
        <activity android:name=".UserInfomationActivity" />
        <activity android:name=".UpdatePasswordActivity" />
        <activity android:name=".NewContactsTypeActivity" />
        <activity android:name=".TypeModifyActivity" />
        <activity android:name=".AddContactsActivity" />
        <activity android:name=".SearchContactsActivity" />
        <activity android:name=".EditContactActivity" />
        <activity android:name=".UpdateContactsActivity" />
        <activity android:name=".DropActivity" />
        <activity android:name=".HelpActivity"></activity>
    </application>

</manifest>